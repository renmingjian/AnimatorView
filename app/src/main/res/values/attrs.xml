<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingView">
        <attr name="dotColor" format="color"/>
        <attr name="dotRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="LongClickView">
        <attr name="bgSrc" format="reference"/>
        <attr name="src" format="reference"/>
    </declare-styleable>

    <declare-styleable name="LoadingButton">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="CheckScoreView">
        <attr name="radius" format="dimension" />
        <attr name="out_layer_length" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ProgressLayoutView">
        <attr name="img" format="reference"/>
        <attr name="imgWidth" format="dimension" />
        <attr name="imgHeight" format="dimension" />
        <attr name="desc" format="string" />
    </declare-styleable>

</resources>